{
  "expo": {
    "name": "LinkLoop",
    "slug": "linkloop-lite",
    "version": "1.0.0",
    "owner": "kcunningham408",
    "description": "T1D glucose tracking with Dexcom CGM integration, Care Circle sharing, and AI-powered insights.",
    "githubUrl": "https://github.com/kcunningham408/linkloop",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A90D9"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vibecmd.linkloop",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4A90D9"
      },
      "package": "com.vibecmd.linkloop",
      "versionCode": 1,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "plugins": [
      "expo-updates"
    ],
    "extra": {
      "eas": {
        "projectId": "fe571b30-5832-4068-b4cd-327399d778c6"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/fe571b30-5832-4068-b4cd-327399d778c6"
    }
  }
}
